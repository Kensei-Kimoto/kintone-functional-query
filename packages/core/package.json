{
  "name": "@kintone-functional-query/core",
  "version": "0.1.0",
  "description": "Type-safe functional query builder for kintone",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "dev": "vitest --watch",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "keywords": [
    "kintone",
    "query",
    "typescript",
    "functional"
  ],
  "author": "kimotokensei",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kimotokensei/kintone-functional-query.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/kimotokensei/kintone-functional-query/issues"
  },
  "homepage": "https://github.com/kimotokensei/kintone-functional-query#readme",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "effect": "^3.0.0"
  },
  "peerDependencies": {
    "kintone-effect-schema": "*"
  }
}
